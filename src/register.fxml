<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="RegisterControl">
   <children>
      <AnchorPane prefHeight="67.0" prefWidth="600.0" style="-fx-background-color: yellow;">
         <children>
            <Label layoutX="212.0" layoutY="15.0" prefHeight="40.0" prefWidth="124.0" text="Register">
               <font>
                  <Font name="System Bold" size="28.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <TextField fx:id="mobilecol" layoutX="90.0" layoutY="213.0" prefHeight="26.0" prefWidth="418.0" promptText="Enter Youer Mobile No" />
      <TextField fx:id="namecol" layoutX="91.0" layoutY="158.0" prefHeight="26.0" prefWidth="418.0" promptText="Enter Your Name" />
      <TextField fx:id="idcol" layoutX="90.0" layoutY="105.0" prefHeight="26.0" prefWidth="418.0" promptText="Enter Youer ID" />
      <Button fx:id="submitcol" layoutX="189.0" layoutY="319.0" mnemonicParsing="false" onAction="#submitButton" prefHeight="26.0" prefWidth="162.0" style="-fx-background-color: yellow;" text=" submit">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Label layoutX="91.0" layoutY="80.0" prefWidth="28.0" text="ID" />
      <Label layoutX="90.0" layoutY="139.0" prefHeight="18.0" prefWidth="52.0" text="name" />
      <Label layoutX="90.0" layoutY="194.0" prefHeight="18.0" prefWidth="52.0" text="mobile" />
      <TextField fx:id="coursecol" layoutX="91.0" layoutY="271.0" prefHeight="26.0" prefWidth="418.0" promptText="Enter Youer Course" />
      <Label layoutX="91.0" layoutY="252.0" prefHeight="18.0" prefWidth="52.0" text="course" />
      <Button fx:id="closecol" layoutX="189.0" layoutY="359.0" mnemonicParsing="false" onAction="#closeButtonOnAction" prefHeight="26.0" prefWidth="162.0" style="-fx-background-color: yellow;" text="close">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Label fx:id="registerMsg" layoutX="244.0" layoutY="80.0" text=" " textFill="#233fbc" />
   </children>
</AnchorPane>
